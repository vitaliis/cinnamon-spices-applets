#: applet.js:1139
msgid "Search Apps..."
msgstr "Søg programmer …"

#. slingshot@jfarthing84->metadata.json->description
msgid ""
"A clone of Pantheon's Slingshot launcher in the form of a Cinnamon applet."
msgstr ""
"En klon af Pantheons Slingshot-programstarter i form af et Cinnamon-"
"panelprogram."

#. slingshot@jfarthing84->metadata.json->name
msgid "Slingshot"
msgstr "Slingshot"

#. slingshot@jfarthing84->settings-schema.json->menu-editor-
#. button->description
msgid "Open the menu editor"
msgstr "Åbn redigering af menuen"

#. slingshot@jfarthing84->settings-schema.json->menu-editor-button->tooltip
msgid "Press this button to customize your menu entries."
msgstr "Tryk på denne knap for at tilpasse menupunkterne."

#. slingshot@jfarthing84->settings-schema.json->menu-icon-custom->description
msgid "Use a custom icon"
msgstr "Anvend et selvvalgt ikon"

#. slingshot@jfarthing84->settings-schema.json->menu-icon-custom->tooltip
msgid "Unchecking this allows the theme to set the icon"
msgstr "Hvis deaktiveret vælger temaet ikonet"

#. slingshot@jfarthing84->settings-schema.json->slingshot-use-
#. category->description
msgid "Use category view"
msgstr "Anvend kategorivisning"

#. slingshot@jfarthing84->settings-schema.json->slingshot-use-
#. category->tooltip
msgid "Default to category view."
msgstr "Som standard kategorivisning."

#. slingshot@jfarthing84->settings-schema.json->slingshot-rows->description
msgid "Rows:"
msgstr "Rækker:"

#. slingshot@jfarthing84->settings-schema.json->slingshot-rows->tooltip
msgid "Set the Slingshot row count."
msgstr "Angiver antal rækker."

#. slingshot@jfarthing84->settings-schema.json->slingshot-rows->units
msgid "rows"
msgstr "rækker"

#. slingshot@jfarthing84->settings-schema.json->slingshot-icon-
#. size->description
msgid "Icon size:"
msgstr "Ikonstørrelse:"

#. slingshot@jfarthing84->settings-schema.json->slingshot-icon-size->tooltip
msgid "Set the Slingshot icon size."
msgstr "Angiver ikonstørrelsen."

#. slingshot@jfarthing84->settings-schema.json->slingshot-icon-size->units
msgid "pixels"
msgstr "pixler"

#. slingshot@jfarthing84->settings-schema.json->menu-icon->description
msgid "Icon"
msgstr "Ikon"

#. slingshot@jfarthing84->settings-schema.json->menu-icon->tooltip
msgid "Select an icon to show in the panel."
msgstr "Vælg et ikon som vises i panelet."

#. slingshot@jfarthing84->settings-schema.json->overlay-key->description
msgid "Keyboard shortcut to open and close Slingshot"
msgstr "Tastaturgenvej til åbning og lukning af Slingshot"

#. slingshot@jfarthing84->settings-schema.json->menu-label->description
msgid "Text"
msgstr "Tekst"

#. slingshot@jfarthing84->settings-schema.json->menu-label->tooltip
msgid "Enter custom text to show in the panel."
msgstr "Indtast selvvalgt tekst som vises i panelet."

#. slingshot@jfarthing84->settings-schema.json->slingshot-columns->description
msgid "Columns:"
msgstr "Kolonner:"

#. slingshot@jfarthing84->settings-schema.json->slingshot-columns->tooltip
msgid "Set the Slingshot column count."
msgstr "Angiver antal kolonner."

#. slingshot@jfarthing84->settings-schema.json->slingshot-columns->units
msgid "columns"
msgstr "kolonner"
