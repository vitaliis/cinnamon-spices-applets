#. stackoverflow-questions-notifier@higuaro->metadata.json->description
msgid ""
"Checks stackoverflow site to see if new questions have been posted under a "
"particular tag and generates a notification"
msgstr ""
"Tjekker stackoverflow-sitet for at se, om nye spørgsmål er slået op med et "
"bestemt mærke, og genererer en meddelelse"

#. stackoverflow-questions-notifier@higuaro->metadata.json->name
msgid "Stackoverflow Questions Notifier"
msgstr "Stackoverflow"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenCinnamon->description
msgid "Visit Cinnamon Applet Home"
msgstr "Besøg Cinnamon-panelprogrammers hjemmeside"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenCinnamon->tooltip
msgid "Open cinnamon.com - rate the applet"
msgstr "Åbn cinnamon.com - bedøm panelprogrammet"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->txtTagList->description
msgid "Tags of interest for new questions (max 10 tags): "
msgstr "Mærker af interesse for nye spørgsmål (maks. 10): "

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->txtTagList->tooltip
msgid ""
"Comma-separated list of tags for new questions that you are interested in "
"(tags beyond the 10th are ignored)"
msgstr ""
"Kommasepareret liste over mærker for nye spørgsmål du er interesseret i "
"(mærker ud over den 10. ignoreres)"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->general->description
msgid "General Settings"
msgstr "Generelle indstillinger"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->scaQueryFrecuency->description
msgid "Query interval (minutes): "
msgstr "Forespørgselsinterval (minutter): "

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->scaQueryFrecuency->tooltip
msgid "Change the amount of time between every check for new questions"
msgstr "Ændr tiden mellem hvert tjek for nye spørgsmål"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenDevPage->description
msgid "Visit Developer Page"
msgstr "Besøg udviklerens side"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenDevPage->tooltip
msgid "Open geekofficedog.blogspot.com"
msgstr "Åbn geekofficedog.blogspot.com"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenStackoverflow->description
msgid "Visit Stackoverflow"
msgstr "Besøg Stackoverflow"

#. stackoverflow-questions-notifier@higuaro->settings-
#. schema.json->btnOpenStackoverflow->tooltip
msgid "Open stackoverflow.com"
msgstr "Åbn stackoverflow.com"
