msgid "Type to search..."
msgstr "键入以搜索..."

#. sane-menu@nooulaif->metadata.json->description
msgid "Cinnamon stock menu applet with improved search."
msgstr "具有改进的搜索的Cinnamon stock菜单。"

#. sane-menu@nooulaif->metadata.json->name
msgid "Sane Menu"
msgstr "Sane菜单"

#. sane-menu@nooulaif->settings-schema.json->menu-editor-button->description
msgid "Open the menu editor"
msgstr "打开菜单编辑器"

#. sane-menu@nooulaif->settings-schema.json->menu-editor-button->tooltip
msgid "Press this button to customize your menu entries."
msgstr "按此按钮自定义您的菜单条目。"

#. sane-menu@nooulaif->settings-schema.json->menu-icon-custom->description
msgid "Use a custom icon"
msgstr "使用自定义图标"

#. sane-menu@nooulaif->settings-schema.json->menu-icon-custom->tooltip
msgid "Unchecking this allows the theme to set the icon"
msgstr "不选中此项允许主题设置图标"

#. sane-menu@nooulaif->settings-schema.json->hover-delay->description
msgid "Menu hover delay:"
msgstr "菜单悬停延迟："

#. sane-menu@nooulaif->settings-schema.json->hover-delay->tooltip
msgid "Delay between switching categories"
msgstr "切换类别间的延迟"

#. sane-menu@nooulaif->settings-schema.json->hover-delay->units
msgid "milliseconds"
msgstr "毫秒"

#. sane-menu@nooulaif->settings-schema.json->search-filesystem->description
msgid "Enable filesystem path entry in search box"
msgstr "在搜索框中启用文件系统路径条目"

#. sane-menu@nooulaif->settings-schema.json->search-filesystem->tooltip
msgid "Allows path entry in the menu search box."
msgstr "在菜单搜索框中允许路径条目。"

#. sane-menu@nooulaif->settings-schema.json->show-application-
#. icons->description
msgid "Show application icons"
msgstr "显示应用程序图标"

#. sane-menu@nooulaif->settings-schema.json->show-application-icons->tooltip
msgid "Choose whether or not to show icons on applications."
msgstr "选择是否显示应用程序的图标。"

#. sane-menu@nooulaif->settings-schema.json->favbox-show->description
msgid "Show favorites and quit options"
msgstr "显示收藏夹和退出选项"

#. sane-menu@nooulaif->settings-schema.json->favbox-show->tooltip
msgid "Choose whether or not to show the left pane of the menu."
msgstr "选择是否显示菜单的左侧窗格。"

#. sane-menu@nooulaif->settings-schema.json->menu-icon->description
msgid "Icon"
msgstr "图标"

#. sane-menu@nooulaif->settings-schema.json->menu-icon->tooltip
msgid "Select an icon to show in the panel."
msgstr "选择要显示在面板中的图标。"

#. sane-menu@nooulaif->settings-schema.json->overlay-key->description
msgid "Keyboard shortcut to open and close the menu"
msgstr "打开和关闭菜单的键盘快捷键"

#. sane-menu@nooulaif->settings-schema.json->menu-label->description
msgid "Text"
msgstr "文本"

#. sane-menu@nooulaif->settings-schema.json->menu-label->tooltip
msgid "Enter custom text to show in the panel."
msgstr "输入要在面板中显示的自定义文本。"

#. sane-menu@nooulaif->settings-schema.json->activate-on-hover->description
msgid "Open the menu when I move my mouse over it"
msgstr "当我移动鼠标到菜单上时打开菜单"

#. sane-menu@nooulaif->settings-schema.json->activate-on-hover->tooltip
msgid "Enable opening the menu when the mouse enters the applet"
msgstr "启用当鼠标进入小程序时打开菜单"

#. sane-menu@nooulaif->settings-schema.json->enable-autoscroll->description
msgid "Enable autoscrolling in application list"
msgstr "在应用程序列表中启用自动滚动"

#. sane-menu@nooulaif->settings-schema.json->enable-autoscroll->tooltip
msgid "Choose whether or not to enable smooth autoscrolling in the application list."
msgstr "选择是否在应用程序列表中启用平滑滚动。"

#. sane-menu@nooulaif->settings-schema.json->show-category-icons->description
msgid "Show category icons"
msgstr "显示类别图标"

#. sane-menu@nooulaif->settings-schema.json->show-category-icons->tooltip
msgid "Choose whether or not to show icons on categories."
msgstr "选择是否显示类别图标。"

#. sane-menu@nooulaif->settings-schema.json->show-places->description
msgid "Show bookmarks and places"
msgstr "显示书签和位置"

#. sane-menu@nooulaif->settings-schema.json->show-places->tooltip
msgid "Choose whether or not to show bookmarks and places in the menu."
msgstr "选择是否在菜单中显示书签和位置。"
